<div class="container">
<h1> Create a New User </h1>
<%= form_for @user do |f| %>
<%= f.label :name %></br>
<%= f.text_field :name %></br>
<%= f.label :email  %></br>
<%= f.text_field :email %></br>
<%= f.label :password %><br>
<%= f.password_field :password %></br>
<%= f.label :password_confirmation %></br>
<%= f.password_field :password_confirmation %></br>
<%= f.submit class: 'btn btn-default'%>
<% end %>
</div>
