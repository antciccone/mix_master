<%= link_to "Artist Index", artists_path%></p>

<h1><%= @artist.name %> </h1>
<h1><%= image_tag @artist.image_path %> </h1>
<p><%= link_to "Edit", edit_artist_path(@artist)%></p>
<p><%= link_to "Delete", artist_path(@artist), method: :delete %></p>
<p><%= link_to "New Song", new_artist_song_path(@artist)%></p>
<% @artist.songs.each do |song| %>
<p><%= song.title%> </p>
<%end%> 
